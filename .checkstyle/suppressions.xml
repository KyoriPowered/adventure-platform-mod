<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "http://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
  <!-- Accessor -->
  <suppress files=".*[\\/]accessor[\\/].*Access\.java" checks="MethodName"/>
  <suppress files=".*[\\/]accessor[\\/].*Access\.java" checks="EmptyLineSeparator"/>

  <!-- No JD required in impl or testmod -->
  <suppress files="net[\\/]kyori[\\/]adventure[\\/]platform[\\/]fabric[\\/]impl[\\/].*" checks="FilteringWriteTag|(?:MissingJavadoc.*)|JavadocPackage"/>
  <suppress files="net[\\/]kyori[\\/]adventure[\\/]platform[\\/]neoforge[\\/]impl[\\/].*" checks="FilteringWriteTag|(?:MissingJavadoc.*)|JavadocPackage"/>
  <suppress files="net[\\/]kyori[\\/]adventure[\\/]platform[\\/]modcommon[\\/]impl[\\/].*" checks="FilteringWriteTag|(?:MissingJavadoc.*)|JavadocPackage"/>
  <suppress files="net[\\/]kyori[\\/]adventure[\\/]platform[\\/]test[\\/]fabric[\\/].*" checks="FilteringWriteTag|(?:MissingJavadoc.*)|JavadocPackage"/>
  <suppress files="net[\\/]kyori[\\/]adventure[\\/]platform[\\/]tester[\\/]neoforge[\\/].*" checks="FilteringWriteTag|(?:MissingJavadoc.*)|JavadocPackage"/>

  <!-- suppress duplicate file due to split package -->
  <suppress files="net[\\/]kyori[\\/]adventure[\\/]platform[\\/]fabric[\\/][^\\/]*" checks="JavadocPackage"/>
  <suppress files="net[\\/]kyori[\\/]adventure[\\/]platform[\\/]modcommon[\\/][^\\/]*" checks="JavadocPackage"/>
</suppressions>
